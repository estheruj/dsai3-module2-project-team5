// dbdiagram.io

Table stg_category_translation {
  product_id string
  product_category_name string
  product_category_name_english string
}

Table stg_customers {
  customer_id string [pk] // Assumed Primary Key
  customer_unique_id string
  customer_zip_code_prefix string
  customer_city string
  customer_state string
}

Table stg_geolocation {
  geolocation_city string
  geolocation_lat string
  geolocation_lng string
  geolocation_state string
  geolocation_zip_code_prefix string
}

Table stg_order_items {
  order_id string
  order_item_id integer
  product_id string
  seller_id string
  shipping_limit_date timestamp
  price numeric
  freight_value numeric
}

Table stg_order_payments {
  order_id string
  payment_sequential integer
  payment_type string
  payment_installments integer
  payment_value numeric
}

Table stg_order_reviews {
  review_id string
  order_id string
  review_score integer
  review_comment_title string
  review_comment_message string
  review_creation_date timestamp
  review_answer_timestamp timestamp
}

Table stg_orders {
  order_id string [pk] // Assumed Primary Key
  customer_id string
  order_status string
  order_purchase_timestamp timestamp
  order_approved_at timestamp
  order_delivered_carrier_date timestamp
  order_delivered_customer_date timestamp
  order_estimated_delivery_date timestamp
}

Table stg_products {
  product_id string [pk] // Assumed Primary Key
  product_category_name string
  product_name_length numeric
  product_description_length numeric
  product_photos_qty numeric
  product_weight_g numeric
  product_length_cm numeric
  product_height_cm numeric
  product_width_cm numeric
}

Table stg_sellers {
  seller_id string [pk] // Assumed Primary Key
  seller_zip_code_prefix string
  seller_city string
  seller_state string
}


Ref: stg_orders.customer_id > stg_customers.customer_id
Ref: stg_order_items.order_id > stg_orders.order_id
Ref: stg_order_items.product_id > stg_products.product_id
Ref: stg_order_items.seller_id > stg_sellers.seller_id
Ref: stg_order_payments.order_id > stg_orders.order_id
Ref: stg_order_reviews.order_id > stg_orders.order_id
Ref: stg_products.product_category_name > stg_category_translation.product_category_name